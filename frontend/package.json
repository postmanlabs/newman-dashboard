{
  "name": "newman-dashboard-frontend",
  "version": "0.0.1",
  "description": "A WebUI companion for Newman to control, view and debug runs.",
  "scripts": {
    "test": "npm run test:lint && npm run test:suite",
    "test:lint": "eslint .",
    "test:suite": "npm run test:unit && npm run test:integration",
    "test:unit": "exit 0",
    "test:integration": "exit 0",
    "coverage": "npm run coverage:unit && npm run coverage:integration",
    "coverage:unit": "exit 0",
    "coverage:integration": "exit 0",
    "dev": "next .",
    "build": "next build . && next export ."
  },
  "author": "elit-altum",
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 12"
  },
  "dependencies": {
    "mobx": "6.3.2",
    "mobx-react": "7.2.0",
    "nanoid": "3.1.20",
    "next": "11.0.1",
    "postman-collection": "4.0.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "socket.io-client": "4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "autoprefixer": "^10.3.1",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "nodemon": "^2.0.7",
    "postcss": "^8.3.5",
    "prettier": "^2.3.0",
    "sass": "1.35.2",
    "tailwindcss": "^2.2.4"
  }
