{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "include": [
            "."
        ],
        "exclude": [
            "node_modules"
        ]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "./node_modules/postman-jsdoc-theme",
        "encoding": "utf8",
        "destination": "./out/docs",
        "recurse": true
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": false
    }
}